<%- include('../partials/header.ejs') %>

    <div class="container text-center">
        <h1 class="display-2 my-5">Enter member details:</h1>
        <form class="text-center" action="/members" method="POST">
            <div class="form-group">
            <label for="fName">First Name: </label>
            <input type="text" class="form-control" id="fName" name="fName">
            </div>
            <div class="form-group">
            <label for="lName">Last Name: </label>
            <input type="text" class="form-control" id="lName" name="lName">
            </div>
            <div class="form-group">
                <label for="mobile">Mobile: </label>
                <input type="text" class="form-control" id="mobile" name="mobile">
            </div>
            <div class="form-group">
                <label for="address">Address: </label>
                <input type="text" class="form-control" id="address" name="address">
            </div>
            <div class="form-group">
                <label for="membershipDOI">Membership Date Of Issue: </label>
                <input type="date" class="form-control" id="membershipDOI" name="membershipDOI">
            </div>
            <div class="form-group">
                <label for="membershipExpiry">Membership Date Of Expiry: </label>
                <input type="date" class="form-control" id="membershipExpiry" name="membershipExpiry">
            </div>
            <button type="submit" class="btn btn-primary">Submit</button>
        </form>
    </div>


<%- include('../partials/footer.ejs') %>
